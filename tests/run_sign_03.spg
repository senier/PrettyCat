<?xml version="1.0"?>
<spg code="testlib">

    <const id="Public key">
        <description>
            Pulic DSA key
        </description>
        <config hexbytes=""/>
        <flow sarg="const" sink="Verify sig" darg="pubkey"/>
    </const>

    <const id="Message">
        <description>
            Input message
        </description>
        <config hexbytes="f7ff9e8b7bb2e09b70935a5d785e0cc5d9d0abf0"/>
        <flow sarg="const" sink="Verify sig" darg="msg"/>
    </const>

    <const id="Signature">
        <description>
            Signature
        </description>
        <!-- First nibble changed from 5 to 4: Must fail! -->
        <config hexbytes="490e973a228fb2db61a326a0650f9b1f60c7de77dc21e4decc7131f210eb1cf5704c6e3ecafaa840"/>
        <flow sarg="const" sink="Verify sig" darg="auth"/>
    </const>

    <verify_sig id="Verify sig">
        <description>
            Compare both values
        </description>
        <flow sarg="result" sink="Check" darg="data"/>
    </verify_sig>

    <env id="Check" code="check_fixed">
        <description>
            Check whether comparison yields false
        </description>
        <config result="False"/>
        <arg name="data"/>
    </env>

</spg>
