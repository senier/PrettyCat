<?xml version="1.0"?>
<spg assert_fail="true">
    <env id="Input" confidentiality="false" integrity="false">
        <description>
            Input not guaranteeing confidentiality
        </description>
        <flow sarg="data" sink="Branch" darg="data"/>
    </env>

    <xform id="Branch" code="branch">
        <description>
            Dummy branch operation
        </description>
        <flow sarg="data" sink="Output" darg="data"/>
        <arg name="data"/>
    </xform>

    <env id="Output" confidentiality="false" integrity="true">
        <description>
            Output guaranteeing confidentiality
        </description>
        <arg name="data"/>
    </env>
</spg>
