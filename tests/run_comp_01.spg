<?xml version="1.0"?>
<spg code="testlib">

    <const id="Left">
        <description>
            Left value
        </description>
        <config string="foobar"/>
        <flow sarg="const" sink="Compare" darg="data1"/>
    </const>

    <const id="Right">
        <description>
            Right value
        </description>
        <config string="foobar"/>
        <flow sarg="const" sink="Compare" darg="data2"/>
    </const>

    <comp id="Compare">
        <description>
            Compare both values
        </description>
        <flow sarg="result" sink="Check" darg="data"/>
    </comp>

    <output id="Check" code="check_fixed">
        <description>
            Check whether comparison yields true
        </description>
        <config result="True"/>
        <arg name="data"/>
    </output>

</spg>
