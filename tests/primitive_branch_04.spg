<?xml version="1.0"?>
<spg>

    <!--
      ** This test must succeed, integrity is guaranteed in Output_Intg1
      ** and Output_Intg2, thus it must be guaranteed in Input, too.
      ** -->

    <input id="Input" integrity="true" confidentiality="false">
        <description>
            Confidential input
        </description>
        <flow sarg="data" sink="Branch" darg="data"/>
    </input>

    <branch id="Branch">
        <description>
            Branch data value
        </description>
        <flow sarg="data#1" sink="Output_Intg" darg="data">
            <assert integrity="true"/>
        </flow>
        <flow sarg="data#2" sink="Output_Nonintg" darg="data">
            <assert integrity="true"/>
        </flow>
    </branch>

    <output id="Output_Intg" integrity="true" confidentiality="false">
        <description>
            Integrity output
        </description>
        <arg name="data"/>
    </output>

    <output id="Output_Nonintg" integrity="true" confidentiality="false">
        <description>
            Non-integrity output
        </description>
        <arg name="data"/>
    </output>
</spg>
