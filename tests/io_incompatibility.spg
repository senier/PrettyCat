<spg>
    <env id="Input" confidentiality="True" integrity="False">
        <description>
            Input guaranteeing confidentiality
        </description>
        <flow sarg="data" sink="Output" darg="data"/>
    </env>

    <env id="Output" confidentiality="False" integrity="False">
        <description>
            Output *not* guaranteeing confidentiality
        </description>
    </env>
</spg>
